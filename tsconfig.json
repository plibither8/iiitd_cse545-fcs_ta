{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": false,
    "outDir": "dist",
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "es6",
    "baseUrl": ".",
    "rootDir": "./scripts",
    "paths": {
      "scripts/*": ["./scripts/*"]
    },
    "types": ["@cloudflare/workers-types"]
  },
  "lib": ["ESNext"],
  "include": ["scripts/**/*.ts"]
}
